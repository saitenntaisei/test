<launch>
    <node name="joy_node" pkg="joy" type="joy_node">
        <param name="/dev" value="/dev/input/js0"/>
    </node>
    <node pkg="controller" name="img_publisher" type="img_publisher">
        <param name="port_name" value="/dev/video0" />
    </node>
    <node pkg="controller" name="img_subscriber" type="img_subscriber"/>
    <!-- <node pkg="controller" name="signal_publisher" type="signal_publisher"/> -->
    <!-- <node pkg="controller" name="signal_subscriber" type="signal_subscriber"/> -->
    <node pkg="joy_simple_operator" type="joy_simple_operator" name="joy_simple_operator_node">
    </node>
    <node pkg="serial_interface" type="serial_interface" name="nucleo_driver" output="screen">
        <param name="port_name" value="/dev/ttyACM0" />
    </node>
</launch>
